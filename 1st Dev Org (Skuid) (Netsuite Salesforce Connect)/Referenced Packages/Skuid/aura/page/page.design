<design:component label="Skuid Page">
	<design:attribute name="page" label="Page Name" description="Name of the Skuid Page you want to use" />
	<design:attribute name="type" label="Page Type" description="The Type of page, Desktop or Mobile"
                      datasource="Desktop,Mobile" />
    <design:attribute name="id" label="Id" description="A shortcut to the commonly used id property."/>
