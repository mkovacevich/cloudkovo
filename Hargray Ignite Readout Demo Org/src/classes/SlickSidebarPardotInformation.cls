public with sharing class SlickSidebarPardotInformation extends SlickSidebarAbstractController {
  public SlickSidebarPardotInformation() {
      
  }

    private String objectName {set;get;}
    public sObject customerInfo {set;get;}

    public List<Schema.FieldSetMember> getFieldSetMembers() {
        if(currLead != null){
            return SObjectType.Lead.FieldSets.SlickSidebarPardotLead.getFields();
        } else if ( currContact == null ) {
		    return SObjectType.Contact.FieldSets.SlickSidebarPardotContact.getFields();                        
        } else {
		  return SObjectType.Account.FieldSets.SlickSidebarPardotAccount.getFields();
        } 
    }

    public String getObjectName() {
        return 'Pardot Information';
    }

    public override void loadWidget() {    
        String queryString;
        if ( currContact != null ) {
            System.debug('SlickSidebarPardotInformation loading PardotContactInformation Fields');
            queryString = SlickSidebarFieldSetHelper.createBaseQueryString(new Contact(), 
				SObjectType.Contact.FieldSets.SlickSidebarPardotContact.getFields(), currContact.Id);
        } 
        //new
        else if( currLead != null ) {
            System.debug('SlickSidebarPardotInformation loading PardotLeadInformation Fields');            
            queryString = SlickSidebarFieldSetHelper.createBaseQueryString(new Lead(), 
				SObjectType.Lead.FieldSets.SlickSidebarPardotLead.getFields(), currLead.Id);            
        } else {
            System.debug('SlickSidebarPardotInformation loading PardotAccountInformation Fields');            
            queryString = SlickSidebarFieldSetHelper.createBaseQueryString(new Account(), 
				SObjectType.Account.FieldSets.SlickSidebarPardotAccount.getFields(), currAccount.Id);            
        } 

        System.debug('SlickSidebarPardotInformation.loadWidget() - query string: ' + queryString); 
        customerInfo = Database.query(queryString);
    }

