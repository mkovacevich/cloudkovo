        queryString = SlickSidebarFieldSetHelper.createBaseQueryString(new SlickSidebar_Related1__c(), SObjectType.SlickSidebar_Related2__c.FieldSets.SlickSidebarRelated2.getFields());

        if ( currCase != null ) {
	        queryString += ' WHERE case__c = \'' + currCase.Id + '\'';                        
        } else if(currContact != null) {
	        queryString += ' WHERE contact__c = \'' + currContact.Id + '\'';                        
        } else {
	        queryString += ' WHERE account__c = \'' + currAccount.Id + '\'';            
        }                 
        System.debug('SlickSidebarRelated1.init() - Service Request query string: ' + queryString); 
        related1 = Database.query(queryString);

        if (related1 != null && related1.size() > 1) {
            objectName = SlickSidebar_Related1__c.sObjectType.getDescribe().getLabelPlural();
        } else {
            objectName = SlickSidebar_Related1__c.sObjectType.getDescribe().getLabel();
        }
    }

