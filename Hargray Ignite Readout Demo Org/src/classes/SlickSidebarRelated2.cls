    public override void loadWidget() {
    
        related2 = new List<SlickSidebar_Related2__c>();
        String queryString;
        queryString = SlickSidebarFieldSetHelper.createBaseQueryString(new SlickSidebar_Related2__c(), SObjectType.SlickSidebar_Related2__c.FieldSets.SlickSidebarRelated2.getFields());

        if ( currContact != null ) {	       
			queryString += ' WHERE contact__c = \'' + currContact.Id + '\'';                                    
        } else if(currCase != null) {
	        queryString += ' WHERE case__c = \'' + currCase.Id + '\'';                        
        } else {
	        queryString += ' WHERE account__c = \'' + currAccount.Id + '\'';            
        }                 
        System.debug('SlickSidebarRelated2.init() - Service Request query string: ' + queryString); 
        related2 = Database.query(queryString);

        if (related2 != null && related2.size() > 1) {
            objectName = SlickSidebar_Related2__c.sObjectType.getDescribe().getLabelPlural();
        } else {
            objectName = SlickSidebar_Related2__c.sObjectType.getDescribe().getLabel();
        }
    }

