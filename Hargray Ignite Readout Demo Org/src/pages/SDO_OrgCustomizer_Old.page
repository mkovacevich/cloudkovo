<apex:page showheader="false" sidebar="false" standardStylesheets="false">
    <title>SDO Org Customizer Old</title>
    <apex:composition template="SDO_Bootstrap_Template"/>
    <apex:form >
        <apex:define name="_content">
            <div class="row-fluid">
                <div class="span12">
                    <div class="well" style="border: 2px solid #049cdb;">
                        <p class="muted pull-right">v3.0</p>
                        <h1>SDO Org Customizer 
                            <span class="label label-important">Advanced Tool</span>
                            <span class="label label-warning">Deprecated</span>
                        </h1>
                    </div>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12">
                    <div id="org_customizer">
                        <object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000"
                         id="FlexSalesforce" width="700" height="458" codebase="https://fpdownload.macromedia.com/get/flashplayer/current/swflash.cab">
                            <param name="movie" value="{!$Resource.org_customizer}" />
                            <param name="quality" value="high" /><param name="play" value="true" />
                            <param name="bgcolor" value="#f3f3ec" />
                            <param name="allowScriptAccess" value="always" />
                            <param name="flashvars" value="session_id={!$Api.Session_ID}&server_url={!$Api.Partner_Server_URL_90}" />
                            
                            <embed src="{!$Resource.org_customizer}" play="true" bgcolor="#f3f3ec"
                                width="700" height="458" name="FlexSalesforce" align="middle"
                                flashvars="session_id={!$Api.Session_ID}&server_url={!$Api.Partner_Server_URL_90}&user_email={!$User.Email}"
                                loop="false" allowScriptAccess="always" type="application/x-shockwave-flash"
                                pluginspage="http://www.adobe.com/go/getflashplayer">
                            </embed> 
                        </object>
                    </div>
                </div>
            </div>
