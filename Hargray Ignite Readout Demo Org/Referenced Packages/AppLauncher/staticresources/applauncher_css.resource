 @media only screen and (min-device-width : 320px) and (max-device-width : 480px) {
          
     .app{
         height: 200px;
         width:100%;
         border-bottom: solid 1px #e4e3e2;
         margin:0px;
         padding-top:20px;
         padding-bottom:20px;
         border-top: solid 0px ;
         border-left: solid 0px ;
         border-right: solid 0px ;
         border-radius: 0px;
         -moz-border-radius: 0px;
         -webkit-border-radius: 0px;
         box-shadow: 0 0px 0px 0px #d7d6d5;
         -moz-box-shadow: 0 0px 0px 0px #d7d6d5;
         -webkit-box-shadow: 0 0px 0px 0px #d7d6d5;
         float: none;
     }
     
     .logo {
         width:300px;
         height:188px;
         background-size:300px 188px;
         margin:0px;
         margin-right:40px;
         float: left;
     }
 
 
     .applabel {
         font-family:Arial,Helvetica,Sans-serif; 
         font-size:50px;
         color: #696e71;
         text-decoration: none;
         background-color:#fff;
         padding-top:65px;
 
     }
     
     #applaunchertitle {
     	font-size:60px;
     	padding: 20px;
     }
     
     #applauncherheader {
     	padding:0px;
     	margin: 0px;
     }
     
     #applaunchersearch {
     	padding-right:40px;
     	padding-left: 20px;
     }
     
     #q {
     	width: 100%;
     	border: 1px solid #grey;
     	font: 46px Arial,Helvetica,Sans-serif;
     	color: #bebebe;
     	float:none;
     }
     
	        
	    #q:focus {
	        width: 100%;
	    }
	    
		       
	    #plus {
	    	padding-top: 0px;
	        font-size:100px;
	    }
	    
	    #getmore {
	        font-size:40px;
	    }
     
     .arrow {
       background:url({!URLFOR($Resource.arrow)}) no-repeat center center;
		  width: 31px;
		  height: 50px;
		  float: right;
		  margin-top:-150px;
		  margin-right:0px;
		}
 }
