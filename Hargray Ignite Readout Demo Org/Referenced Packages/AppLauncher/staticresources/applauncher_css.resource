
	#appcenter {
	    margin-left:auto;margin-right:auto;
	}
	    
 #applauncherheader{
     padding-top:20px;
     padding-left:20px;
     padding-right:20px;
     padding-bottom:2px;
     text-align:right;
 }
 

	#appcontainer{
	}


 #applaunchertitle {
     font-family:Arial,Helvetica,Sans-serif; 
     font-size:25px;
     color: #696e71;
     float:left;
 }
     
 #q {
     border: 1px solid #ccc;
     font: 12px Arial,Helvetica,Sans-serif;
     color: #bebebe;
     width:216px;
     padding: 6px 6px 6px 12px;
     border-radius: 4px;
     -webkit-border-radius: 4px;
     -moz-border-radius: 4px;
     box-shadow: 0 1px 2px rgba(0, 0, 0, 0.15) inset;
     -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.15) inset;
     -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.15) inset;
     outline: none; 
 }

 .app{
     width:234px;
     margin:20px;
     border: 1px solid #cfd4d9;
     border-radius: 3px;
     -moz-border-radius: 3px;
     -webkit-border-radius: 3px;
     box-shadow: 0 1px 1px 1px #d7d6d5;
     -moz-box-shadow: 0 1px 1px 1px #d7d6d5;
     -webkit-box-shadow: 0 1px 1px 1px #d7d6d5;
     float:left;
 }
 
 .app:hover{
  
 	border: 1px solid #bbc0cd;

 }

 .tile-img {
 	margin-left:17px;
     margin-right:17px;
     margin-top:10px;
     margin-bottom:5px;
 }
 
.tile-img img {
     display: inline;
     max-height: 100%;
     max-width: 100%;
     height: auto;
     width: auto;
     vertical-align: bottom;
 }
     
 .tile-img-inner {
     overflow: hidden;
     display: table-cell;
     text-align: center;
     vertical-align: middle;
     border: 0;
     width: 200px;
     height: 125px;
 }
 
 .applabel {
 	font-family:Arial,Helvetica,Sans-serif; 
     font-size:16px;
     color: #696e71;
     background-color:#e8eaeb;
     height:28px;
     padding-top:14px;
     padding-left: 15px;
     text-decoration: none;
 
 	overflow:hidden;
 	display:block;
 	text-overflow: ellipsis;
 	white-space: nowrap;
 }
 
 .applabel a {
     text-decoration: none;
 }
 
 #apps {padding-bottom:20px;}
 #apps a:link {text-decoration: none;}
 #apps a:visited {text-decoration: none;}
 #apps a:hover {text-decoration: none;}
 

 #getmorewrapper {
     height:181px;
     background-color:#f8f8f8;
 }
 
 #plus {
     padding-top:45px;
     font-family:Arial,Helvetica,Sans-serif; 
     font-size:50px;
     color: #696e71;
     width:100%;
     text-align:center;
 }
 
 #getmore {
     font-family:Arial,Helvetica,Sans-serif; 
     font-size:20px;
     color: #696e71;
     width:100%;
     text-align:center;
 }
	    
	
