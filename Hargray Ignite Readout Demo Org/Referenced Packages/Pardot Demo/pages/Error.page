<apex:page sidebar="false" showHeader="false" controller="salesedgedemo.BaseController">
  <head>
      <apex:stylesheet value="{!URLFOR($Resource.salesedgedemo__Error, 'css/error.css')}" />
      <apex:stylesheet value="{!URLFOR($Resource.salesedgedemo__Error, 'assets/font-awesome/css/font-awesome.min.css')}" />
      <apex:includeScript value="{!URLFOR($Resource.salesedgedemo__Error, 'lib/jquery/jquery.js')}"/>
      <meta charset="utf-8" />
      <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
      <title>Error</title>
      <meta name="description" content="" />
      <meta name="viewport" content="width=device-width, initial-scale=1" />
  </head>
  <body>
      <div class="main-content rounded-corners-3">
          <apex:image styleClass="img-logo" alt="Pardot Logo" url="{!URLFOR($Resource.salesedgedemo__Error, 'img/logo.png')}" />
          <div class="login">
              <div class="user-info">
                  <img class="img-thumbnail pull-left js-thumbnail js-loggedin js-expired" style="display:none" />
                  <p class="js-username js-loggedin js-expired"></p>
                  <button class="btn-pd btn-primary-pd btn-block-pd rounded-corners-3 return-home">Return to home page</button>
              </div>
              <p class="js-loggedout js-loggedin js-expired clear-both">Please contact your salesforce administrator if you have any issues or questions.</p>
              <hr class="shorten"/>
              <div class="message-body"></div>
          </div>
      </div>
      <script type="text/javascript">
        window.CurrentType = '{!$CurrentPage.parameters.type}';
        window.BaseUrl = '{!baseUrl}';
      </script>
      <apex:includeScript value="{!URLFOR($Resource.salesedgedemo__Error, 'js/error_main.js')}"/>
  </body>
</apex:page>